{
  "extends": "@vue/tsconfig/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "isolatedModules": true,
    "esModuleInterop": true,
    "strict": true,
    "moduleResolution": "node",
    "target": "ESNext",
    "module": "ESNext",
    "lib": ["ESNext", "dom"],
    "types": ["cypress", "node"],
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "include": ["**/*.ts"]
}